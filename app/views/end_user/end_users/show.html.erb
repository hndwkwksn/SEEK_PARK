<h1>EndUsers#show</h1>

<table class='table'>
  <tr>
    <th>
      <%= image_tag @end_user.get_profile_image(500,500) %>
    </th>
  </tr>
  <tr>
	<th>name</th>
	<th><%= @end_user.name %></th>
  </tr>
  <tr>
	<th>introduction</th>
	<th><%= @end_user.introduction %></th>
  </tr>
</table>

<div class='row'>
  <% if current_end_user == @end_user %>
    <%= link_to "edit", edit_end_user_path(@end_user) %>
  <% end %>
</div>
