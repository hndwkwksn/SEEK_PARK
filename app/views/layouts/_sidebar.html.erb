<div class="col-md-3 sidebar">
  <div class="sidebox">
    <div class="side-menue">
      <%= link_to parks_path,class: "bn3637 bn38 link-darkmode" do %>
        <i class="fas fa-leaf"></i>
        <span>みんなの投稿</span>
      <% end %>
    </div>
    <div class="side-menue">
      <%= link_to end_user_path(current_end_user),class: "bn3637 bn38 link-darkmode" do %>
        <i class="fas fa-home"></i>
        <span>マイページ</span>
      <% end %>
    </div>
    <div class="side-menue">
      <%= link_to end_user_followings_path(current_end_user),class: "bn3637 bn38 link-darkmode" do %>
        <i class="fas fa-user-friends"></i>
        <span>フォロー中</span>
      <% end %>
    </div>
    <div class="side-menue">
      <%= link_to end_user_followers_path(current_end_user),class: "bn3637 bn38 link-darkmode" do %>
        <i class="fas fa-handshake"></i>
        <span>フォロワー</span>
      <% end %>
    </div>
    <div class="side-menue">
      <%= link_to favorites_end_user_path(current_end_user),class: "bn3637 bn38 link-darkmode" do %>
        <i class="fas fa-heart"></i>
        <span>お気に入り記事</span>
      <% end %>
    </div>
    <div class="side-menue">
      <%= link_to new_park_path, class: "newpost bn3637 bn38 link-darkmode", data: {"turbolinks" => false} do %>
        <span>投稿</span>
      <% end %>
